		"@playwright/test": "^1.28.1",
		"@rokkit/chart": "workspace:*",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@unocss/core": "^0.50.0",
		"@unocss/preset-icons": "^0.50.1",
		"@unocss/preset-uno": "^0.50.1",
		"@unocss/reset": "^0.50.1",
		"@unocss/transformer-directives": "^0.50.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"typescript": "^4.9.3",
		"unocss": "^0.50.0",
		"vite": "^4.0.0",
